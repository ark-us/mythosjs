{
  "name": "@ark-us/wasmxjs",
  "version": "0.0.14",
  "description": "Typescript Protobuf Messages for Mythos",
  "author": "Loredana Cirstea <loredana.cirstea@gmail.com>",
  "homepage": "https://github.com/ark-us/wasmxjs#readme",
  "license": "SEE LICENSE IN LICENSE",
  "main": "main/index.js",
  "module": "module/index.js",
  "browser": "dist/wasmxjs.umd.min.js",
  "unpkg": "dist/wasmxjs.umd.min.js",
  "jsdelivr": "dist/wasmxjs.umd.min.js",
  "typings": "types/index.d.ts",
  "directories": {
    "lib": "src",
    "test": "__tests__"
  },
  "files": [
    "types",
    "src",
    "main",
    "module",
    "dist"
  ],
  "scripts": {
    "build:main": "cross-env BABEL_ENV=production babel src --out-dir main --delete-dir-on-start --extensions \".tsx,.ts,.js\"",
    "build:module": "cross-env MODULE=true babel src --out-dir module --delete-dir-on-start  --extensions \".tsx,.ts,.js\"",
    "build:ts": "tsc --project ./tsconfig.json",
    "build:browser": "rollup -c rollup.config.js",
    "build:browser:esm": "NODE_OPTIONS='--max-old-space-size=8192' rollup -c rollup.config.esm.js",
    "build": "npm run build:module && npm run build:main",
    "buidl": "npm run build && npm run build:ts && npm run build:browser",
    "codegen": "cross-env NODE_ENV=development babel-node scripts/codegen.js",
    "prepare": "npm run build",
    "dev": "cross-env NODE_ENV=development babel-node src/index --extensions \".tsx,.ts,.js\"",
    "watch": "cross-env NODE_ENV=development babel-watch src/index --extensions \".tsx,.ts,.js\"",
    "lint": "eslint src --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect node_modules/.bin/jest --runInBand"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ark-us/wasmxjs"
  },
  "keywords": [],
  "bugs": {
    "url": "https://github.com/ark-us/wasmxjs/issues"
  },
  "devDependencies": {
    "@babel/cli": "7.19.3",
    "@babel/core": "7.19.3",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/node": "^7.19.1",
    "@babel/plugin-proposal-class-properties": "7.18.6",
    "@babel/plugin-proposal-export-default-from": "7.18.10",
    "@babel/plugin-proposal-nullish-coalescing-operator": "7.18.6",
    "@babel/plugin-proposal-numeric-separator": "7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "7.19.4",
    "@babel/plugin-proposal-optional-chaining": "7.18.9",
    "@babel/plugin-transform-runtime": "7.19.1",
    "@babel/preset-env": "7.19.4",
    "@babel/preset-typescript": "^7.18.6",
    "@osmonauts/telescope": "^0.78.0",
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-terser": "^0.4.0",
    "@rollup/plugin-typescript": "^11.0.0",
    "@types/jest": "^29.1.2",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "29.1.2",
    "babel-watch": "^7.0.0",
    "cross-env": "^7.0.2",
    "eslint": "8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.1.2",
    "jest-in-case": "^1.0.2",
    "long": "^4.0.0",
    "prettier": "^2.7.1",
    "regenerator-runtime": "^0.13.7",
    "rimraf": "^3.0.2",
    "rollup": "^4.0.0",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "ts-jest": "^29.0.3",
    "tslib": "^2.6.0",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "@babel/runtime": "^7.19.4",
    "@cosmjs/amino": "^0.30.0",
    "@cosmjs/proto-signing": "^0.30.0",
    "@cosmjs/stargate": "^0.30.0",
    "@cosmjs/tendermint-rpc": "^0.30.0",
    "@osmonauts/lcd": "^0.10.0",
    "protobufjs": "^6.11.2"
  }
}
